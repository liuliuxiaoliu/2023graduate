<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.gejuyuan.mapper.PostMapper">

    <select id="getPostList" resultType="com.example.gejuyuan.entity.Post">
        select * from post
        order by `salary` asc
    </select>

</mapper>